{
  "name": "markdowns-to-single-html",
  "description": "",
  "version": "0.0.6",
  "author": "srymh",
  "files": [
    "/dist-cjs",
    "/dist-esm",
    "/template"
  ],
  "keywords": [],
  "license": "MIT",
  "main": "dist-cjs/index.js",
  "module": "dist-esm/index.js",
  "private": true,
  "scripts": {
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "tsc --project . --module es2015 --outDir ./dist-esm",
    "prebuild": "rimraf ./dist-*",
    "prepare": "npm run build",
    "test": "jest --watch",
    "test:template": "concurrently \"ts-node ./template-test/setup-test.ts\" \"wait-on http://localhost:5000 && wdio run ./template-test/wdio.conf.js\""
  },
  "types": "dist-cjs/index.d.ts",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.10",
    "@types/markdown-it": "^12.0.1",
    "@wdio/cli": "^7.5.3",
    "@wdio/jasmine-framework": "^7.5.3",
    "@wdio/local-runner": "^7.5.5",
    "@wdio/spec-reporter": "^7.5.3",
    "chromedriver": "^90.0.0",
    "concurrently": "^6.0.2",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5",
    "wait-on": "^5.3.0",
    "wdio-chromedriver-service": "^7.0.0",
    "webdriverio": "^7.6.0"
  },
  "dependencies": {
    "jsdom": "^16.5.3",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^7.1.0"
  }
}
